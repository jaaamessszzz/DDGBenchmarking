<ROSETTASCRIPTS>
    <SCOREFXNS>
        <fa_talaris2014 weights=talaris2014/>
    </SCOREFXNS>
    <RESIDUE_SELECTORS>
        <Neighborhood name=bubble resnums=%%target%% distance=8/>
    </RESIDUE_SELECTORS>
    <TASKOPERATIONS>
        <OperateOnResidueSubset name=subset selector=bubble>
            <RestrictToRepackingRLT/>
        </OperateOnResidueSubset>
        <ReadResfile name=readres filename=%%resfile_relpath%%/mutations.resfile/>
    </TASKOPERATIONS>
    <FILTERS>
    </FILTERS>
    <MOVERS>
        <MinMover name=minimize scorefxn=fa_talaris2014 chi=1 bb=1 type=dfpmin_armijo_nonmonotone tolerance=0.001 max_iter=500/>
        <PackRotamersMover name=repack scorefxn=fa_talaris2014 task_operations=subset/>
        <PackRotamersMover name=mutate scorefxn=fa_talaris2014 task_operations=readres/>
        <Backrub name=backrub pivot_residues=%%pivot_residues%%/>
        <ddG name=ddg scorefxn=fa_talaris2014 chain_name=%%chain%%/>
        <SavePoseMover name=savepose restore_pose=0 reference_name=backrubpdb/>
        <SavePoseMover name=restorepose restore_pose=1 reference_name=backrubpdb/>
    </MOVERS>
    <APPLY_TO_POSE>
    </APPLY_TO_POSE>
    <PROTOCOLS>
      <Add mover_name = minimize/>
      <Add mover_name = backrub/>
      <Add mover_name = savepose/>
      <Add mover_name = repack/>
      <Add mover_name = minimize/>
      <Add mover_name = ddg/>
      <Add mover_name = restorepose/>  
      <Add mover_name = mutate/>
      <Add mover_name = repack/>
      <Add mover_name = minimize/>
      <Add mover_name = ddg/>
    </PROTOCOLS>
    <OUTPUT />
</ROSETTASCRIPTS>